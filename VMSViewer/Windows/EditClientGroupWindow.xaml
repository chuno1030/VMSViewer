<Window x:Class="VMSViewer.EditClientGroupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VMSViewer"
        mc:Ignorable="d"
        Title="그룹생성" Height="350" Width="300"
        WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow"
        Loaded="Window_Loaded" Closing="Window_Closing"
        Background="{DynamicResource BackGround_Color}">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <TextBox x:Name="txtGroupName" Width="150" Height="25"/>
            <Button x:Name="btnSave" Width="50" Height="25" Click="btnClick"/>
        </StackPanel>
    </Grid>
</Window>
